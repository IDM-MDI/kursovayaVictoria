<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="course.by.zhukova.entity.CartEntity" table="cart" schema="shoes_shop">
        <id name="idcart">
            <column name="idcart" sql-type="int"/>
            <generator class="increment"/>
        </id>
        <property name="cartUserId">
            <column name="cartUserID" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>