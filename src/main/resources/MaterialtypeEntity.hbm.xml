<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="course.by.zhukova.entity.MaterialtypeEntity" table="materialtype" schema="shoes_shop">
        <id name="idmaterialtype">
            <column name="idmaterialtype" sql-type="int"/>
            <generator class="increment"/>
        </id>
        <property name="materialtypeName">
            <column name="materialtypeName" sql-type="varchar(45)" length="45"/>
        </property>
        <set name="materialsByIdmaterialtype" inverse="true">
            <key>
                <column name="materialInside"/>
            </key>
            <one-to-many not-found="ignore" class="course.by.zhukova.entity.MaterialEntity"/>
        </set>
        <set name="materialsByIdmaterialtype_0" inverse="true">
            <key>
                <column name="materialOutside"/>
            </key>
            <one-to-many not-found="ignore" class="course.by.zhukova.entity.MaterialEntity"/>
        </set>
        <set name="materialsByIdmaterialtype_1" inverse="true">
            <key>
                <column name="materialSole"/>
            </key>
            <one-to-many not-found="ignore" class="course.by.zhukova.entity.MaterialEntity"/>
        </set>
        <set name="materialsByIdmaterialtype_2" inverse="true">
            <key>
                <column name="materialUpper"/>
            </key>
            <one-to-many not-found="ignore" class="course.by.zhukova.entity.MaterialEntity"/>
        </set>
    </class>
</hibernate-mapping>